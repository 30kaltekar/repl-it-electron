{
    "name": "repl.it",
    "version": "1.1.0",
    "description": "electron version of repl.it",
    "main": "main.js",
    "scripts": {
        "lint": "eslint \"**/*.{js,ts}\"",
        "start": "npm run build2 && npm run build2:run",
        "build2:tsc": "tsc -b tsconfig.json",
        "build2": "gulp --log-level3",
        "build2:run": "electron ./ts-out/"
    },
    "keywords": [
        "electron",
        "repl.it",
        "discord-rpc"
    ],
    "author": "leon332157&repl-it-discord",
    "devDependencies": {
        "@types/gulp": "^4.0.6",
        "@types/node": "^12.12.21",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "acorn": "^7.1.1",
        "clean-webpack-plugin": "^3.0.0",
        "electron": "^8.2.2",
        "electron-context-menu": "^1.0.0",
        "electron-packager": "^14.2.1",
        "electron-prompt": "^1.5.1",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-watch": "^6.0.1",
        "eventemitter2": "^6.3.1",
        "gulp": "^4.0.2",
        "gulp-typescript": "^6.0.0-alpha.1",
        "nodemon": "^2.0.3",
        "prettier": "^2.0.4",
        "restler": "^3.4.0",
        "ts-node": "^8.8.2",
        "tslint": "^6.1.1",
        "typescript": "^3.8.3",
        "yarn": "^1.22.4",
        "zip-local": "^0.3.4"
    },
    "dependencies": {
