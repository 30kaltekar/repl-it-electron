{
    "name": "repl.it",
    "version": "1.1.0",
    "description": "electron version of repl.it",
    "main": "main.js",
    "scripts": {
        "start": "npm run dev:main",
        "dev:main": "concurrently \"npm:dev:main:webpack\" \"npm:dev:main:electron\"",
        "dev:main:electron": "nodemon -w dist/dev -e js,ts,json,html,css --exec electron dist/dev/main.bundle.js",
        "dev:main:webpack": "webpack --config webpack.config.js",
        "lint:fix": "eslint \"src/**/*.{js,ts,json}\" --fix",
        "package": "node src/utils/building.js"
    },
    "keywords": [
        "electron",
        "repl.it",
        "discord-rpc"
    ],
    "author": "leon332157&repl-it-discord",
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/plugin-transform-async-to-generator": "^7.4.4",
        "@babel/plugin-transform-regenerator": "^7.4.4",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-typescript": "^7.3.3",
        "@types/node": "^12.0.0",
        "@typescript-eslint/eslint-plugin": "^1.8.0",
        "@typescript-eslint/parser": "^1.8.0",
        "acorn": "^6.1.1",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^2.0.2",
        "core-js": "^3.0.1",
        "discord-rpc": "^3.0.1",
        "electron": "^5.0.1",
        "electron-context-menu": "^0.10.1",
        "electron-packager": "^13.1.1",
        "electron-prompt": "^1.3.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.2.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-watch": "^5.1.2",
        "eventemitter2": "^5.0.1",
        "express": "^4.16.4",
        "fork-ts-checker-webpack-plugin": "^1.3.1",
        "mobile-detect": "^1.4.3",
        "nodemon": "^1.19.0",
        "prettier": "^1.17.0",
        "restler": "^3.4.0",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.2",
        "zip-local": "^0.3.4"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "concurrently": "^4.1.0",
        "fs-extra": "^5.0.0",
        "lodash": "^4.17.4",
        "node-fetch": "^2.5.0"
    },
    "repository": "https://github.com/repl-it-discord/repl.it-electron",
    "license": "Apache-2.0"
}
