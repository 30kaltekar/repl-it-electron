{
    "name": "repl.it",
    "version": "1.1.0",
    "description": "electron version of repl.it",
    "main": "main.js",
    "scripts": {
        "start": "npm run dev:main",
        "dev:main": "concurrently \"npm:dev:main:webpack\" \"npm:dev:main:electron\"",
        "dev:main:electron": "nodemon -w dist/dev -e js,ts,json,html,css --exec electron dist/dev/main.bundle.js",
        "dev:main:webpack": "webpack --config webpack.config.js",
        "package": "node src/utils/building.js",
        "test": "jest --debug --detectOpenHandles"
    },
    "keywords": [
        "Electron",
        "repl.it",
        "discord-rpc"
    ],
    "author": "leon332157&repl.it-discord",
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/plugin-transform-async-to-generator": "^7.4.4",
        "@babel/plugin-transform-regenerator": "^7.4.4",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-typescript": "^7.3.3",
        "@types/node": "^12.0.0",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^2.0.2",
        "codecov": "^3.2.0",
        "concurrently": "^4.1.0",
        "electron": "^4.1.0",
        "electron-packager": "^13.1.1",
        "express": "^4.16.4",
        "fork-ts-checker-webpack-plugin": "^1.3.1",
        "jest": "^24.3.1",
        "mobile-detect": "^1.4.3",
        "prettier": "^1.16.4",
        "typescript": "^3.4.5",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.2"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "discord-rpc": "^3.0.1",
        "electron-context-menu": "^0.10.1",
        "electron-prompt": "^1.3.0",
        "eventemitter2": "^5.0.1",
        "fs-extra": "^5.0.0",
        "lodash": "^4.17.4",
        "restler": "^3.4.0",
        "zip-local": "^0.3.4"
    },
    "repository": "https://github.com/repl-it-discord/repl.it-electron",
    "license": "Apache-2.0",
    "jest": {
        "verbose": true,
        "testPathIgnorePatterns": [
            "/WorkInProgress/",
            "/dev.obj/"
        ],
        "collectCoverage": true,
        "coverageDirectory": "./coverage/",
        "cache": false,
        "watchman": false,
        "notify": true,
        "useStderr": true
    },
    "browserslist": [
        "Electron > 4"
    ]
}
